<div id="calendar" class="w-100"></div></div><script src="https://cdn.jsdelivr.net/npm/fullcalendar-scheduler@6.1.11/index.global.min.js"></script><script>    var calendar;    document.addEventListener('DOMContentLoaded', function () {        var calendarEl = document.getElementById('calendar');        calendar = new FullCalendar.Calendar(calendarEl, {            height: 'auto',            schedulerLicenseKey: 'GPL-My-Project-Is-Open-Source',            themeSystem: 'bootstrap5',            headerToolbar: false,            initialView: 'resourceTimeline',            datesSet: function (info) {                var title = info.view.title;                $("#calendarTitle").html(title);            },            resources: [                { id: 'a', title: 'Room A' },                { id: 'b', title: 'Room B' },                { id: 'c', title: 'Room C' }            ],            events: [                { id: '1', resourceId: 'a', title: 'Meeting 1', start: '2024-03-28T08:00:00', end: '2024-03-28T10:00:00' },                { id: '2', resourceId: 'b', title: 'Meeting 2', start: '2024-03-28T09:00:00', end: '2024-03-28T11:00:00' },                { id: '3', resourceId: 'c', title: 'Meeting 3', start: '2024-03-28T10:00:00', end: '2024-03-28T12:00:00' }            ]        });        calendar.render();    });    let calendarNext = () => calendar.next();    let calendarPrev = () => calendar.prev();    let calendarToday = () => calendar.today();    let changeView = (type) => calendar.changeView(type);</script>